<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.phonegap.example"
        versionCode = "10" 
        version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->

    <name>Martin Example</name>

    <description>
        An example for phonegap build docs. 
    </description>

    <author href="https://build.phonegap.com" email="support@phonegap.com">
        Hardeep Shoker 
    </author>



<platform name="android">
    <!-- you can use any density that exists in the Android project -->
    <splash src="images/inloggen.png" density="land-hdpi"/>
    <splash src="images/inloggen.png" density="land-ldpi"/>
    <splash src="images/inloggen.png" density="land-mdpi"/>
    <splash src="images/inloggen.png" density="land-xhdpi"/>

    <splash src="images/inloggen.png" density="port-hdpi"/>
    <splash src="images/inloggen.png" density="port-ldpi"/>
    <splash src="images/inloggen.png" density="port-mdpi"/>
    <splash src="images/inloggen.png" density="port-xhdpi"/>
</platform>

<preference name="SplashScreenDelay" value="10000" />


	<icon src="icon.png" />
	<gap:plugin name="org.apache.cordova.vibration" version="0.3.11" />
	<gap:plugin name="nl.x-services.plugins.flashlight" version="2.0.1" />
	<gap:plugin name="com.brodysoft.sqliteplugin" version="1.0.0" />
	<gap:plugin name="com.footprint.cordova.plugin.local-notification" version="0.8.0" />
	<gap:plugin name="hu.dpal.phonegap.plugins.uniquedeviceid" version="1.1.0" />
</widget>
